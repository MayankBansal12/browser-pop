{
  "name": "browser-pop",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "check": "biome check --write .",
    "dev": "turbo dev",
    "build": "turbo build",
    "build:web": "turbo build -F web",
    "build:extension": "turbo build -F extension",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:extension": "turbo -F extension dev",
    "dev:server": "turbo -F @browser-pop/backend dev",
    "dev:setup": "turbo -F @browser-pop/backend dev:setup"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^2.5.4",
    "@biomejs/biome": "^2.2.0"
  },
  "packageManager": "pnpm@10.10.0"
}
